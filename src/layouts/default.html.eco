<html>
<head>
	<title><%= @document.title %></title>
	<%- @getBlock("meta").toHTML() %>
	<%- @getBlock("styles").add(["/assets/styles/style.css"]).toHTML() %>
	<script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
	<script type="text/javascript">
		if (typeof jQuery == 'undefined') {
		    document.write(unescape("%3Cscript src='/assets/js/jquery-1.9.1.min.js' type='text/javascript'%3E%3C/script%3E"));
		}
	</script>	
</head>
<body>
	<div id="content">
		<div id="container">
			<div id="header">
				<a class="header-element" id="header-logo" href="/">
					<img alt="StudyMesh" src="/assets/images/studymeshlogo.jpg"/>
				</a>
				<div class="header-element navbar" id="login-register-bar">
					<ul>
						<li>
							<a id="register-link" href="/">
								Register
							</a>
						</li>
						<li>
							<a id="login-link" href="/loginregister/login">
								Login
							</a>
						</li>
					</ul>
				</div>
				<div class="clear"></div>
			</div>
			<div id="main">
				<%- @content %>
			</div>
			<div id="footer">
				<ul class="menubar">
					<li><a href="/about">About Us</a></li>
					<li><a href="http://blog.studymesh.com">Blog</a></li>
					<li><a href="/pricing">Plans &amp; Pricing</a></li>
					<li><a href="/legal/tos">Terms of Service</a></li>
					<li><a href="/legal/privacy">Privacy</a></li>
					<li><a href="/about/contact">Contact</a></li>
				</ul>
			</div>
		</div>
	</div>
	<%- @getBlock("scripts").add(["/assets/js/ga.js"]).toHTML() %>
</body>
</html>